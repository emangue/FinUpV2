    "venv": "/Users/emangue/Documents/ProjetoVSCode/ProjetoFinancasV3/venv"
  },
  "servers": {
    "backend": {
      "tech": "FastAPI + SQLAlchemy",
      "port": 8000,
      "url": "http://localhost:8000",
      "docs": "http://localhost:8000/docs",
      "start_command": "PYTHONPATH=/Users/emangue/Documents/ProjetoVSCode/ProjetoFinancasV3/app_dev/backend:/Users/emangue/Documents/ProjetoVSCode/ProjetoFinancasV3/codigos_apoio /Users/emangue/Documents/ProjetoVSCode/ProjetoFinancasV3/venv/bin/uvicorn app.main:app --host 0.0.0.0 --port 8000 --reload"
    },
    "frontend": {
      "tech": "Next.js 16.1.1",
      "port": 3000,
      "url": "http://localhost:3000",
      "start_command": "npm run dev"
    }
  },
  "authentication": {
    "required_user_id": 1,
    "credentials": {
      "email": "admin@example.com",
      "password": "admin123",
      "user_id": 1
    }
  },
  "quick_start": {
    "automatic": "./start_servers.sh",
    "stop": "./stop_servers.sh",
    "guide": "GUIA_SERVIDORES.md"
  },
  "database": {
    "type": "SQLite",
    "location": "/Users/emangue/Documents/ProjetoVSCode/ProjetoFinancasV3/app_dev/backend/database/financas_dev.db",
    "key_tables": [
      "users",
      "journal_entries", 
      "base_padroes",
      "preview_transacoes",
      "bank_format_compatibility"
    ]
  },
  "classification": {
    "engine": "CascadeClassifier",
    "levels": 6,
    "level_3": "Base_Padroes with segmented patterns",
    "test_script": "test_sistema_completo.py"
  },
  "troubleshooting": {
    "port_conflicts": {
      "backend": "lsof -i:8000",
      "frontend": "lsof -i:3000"
    },
    "clear_cache": "rm -rf app_dev/frontend/.next",
    "check_db": "sqlite3 database/financas_dev.db '.tables'",
    "verify_user": "sqlite3 database/financas_dev.db 'SELECT * FROM users WHERE id = 1;'"
  }
}